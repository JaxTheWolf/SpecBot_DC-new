{
  "extends":"standard",
  "parser":"babel-eslint",
  "plugins":[
    "eslint-plugin-json",
    "eslint-plugin-node",
    "eslint-plugin-promise",
    "eslint-plugin-import",
    "eslint-plugin-require-sort",
    "eslint-plugin-sort-requires",
    "eslint-plugin-sort-keys-fix"
  ],
  "env":{
    "es6":true,
    "node":true
  },
  "rules":{
    "indent":[
      "warn",
      2
    ],
    "prefer-const":[
      "warn",
      {
        "destructuring":"any",
        "ignoreReadBeforeAssign":false
      }
    ],
    "semi":[
      "error",
      "never"
    ],
    "no-unused-vars":"warn",
    "quotes":[
      "error",
      "backtick"
    ],
    "sort-imports":[
      "warn",
      {
        "ignoreCase":false,
        "ignoreMemberSort":false,
        "memberSyntaxSortOrder":[
          "none",
          "all",
          "multiple",
          "single"
        ]
      }
    ],
    "sort-keys-fix/sort-keys-fix":[
      "warn",
      "asc",
      {
        "caseSensitive":true,
        "natural":true
      }
    ],
    "require-sort/require-sort":[
      "warn",
      {
        "ignoreCase":true,
        "ignoreDeclarationSort":false,
        "ignorePropertySort":false,
        "propertySyntaxSortOrder":[
          "none",
          "single",
          "multiple"
        ]
      }
    ],
    "sort-requires/sort-requires":"warn"
  }
}